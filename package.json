{
  "name": "cap-parser",
  "version": "0.0.1-beta.1",
  "description": "",
  "main": "lib/parser.js",
  "dependencies": {
    "chalk": "^1.1.3",
    "edp-core": "^1.0.32"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-istanbul": "^0.8.0",
    "babel-node-debug": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "debug": "^2.2.0",
    "json-stringify-safe": "^5.0.1",
    "mocha": "^2.5.3"
  },
  "scripts": {
    "start": "npm run compile && DEBUG=cap-parser* node lib/index.js",
    "debug": "npm run compile && DEBUG=cap-parser* ./node_modules/.bin/babel-node-debug lib/index.js",
    "test": "npm run start && ./node_modules/.bin/_mocha --compilers js:babel-core/register --recursive",
    "coverage": "npm run start && ./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover _mocha 'test/**/*.spec.@(js|es|es6)'",
    "coverage1": "npm run start && ./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- --recursive",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "compile": "rm -rf lib && ./node_modules/.bin/babel src/ -d lib/",
    "sourcemap": "./node_modules/.bin/babel src/ -d lib/ -s",
    "watch": "./node_modules/.bin/babel -w src/ -d lib/",
    "prepublish": "npm run compile"
  },
  "author": "ielgnaw",
  "license": "ISC"
}
